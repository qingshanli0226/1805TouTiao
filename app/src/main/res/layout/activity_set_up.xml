<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.SetUpActivity">
    <com.example.framework.view.ToolBar
        android:layout_width="match_parent"
        android:layout_height="@dimen/toolbar_height"
        app:title_show="false"
        app:left_src="@drawable/back"
        android:background="#FF1162"
        app:right_src="@drawable/ic_share_white_24dp">
        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="设置"
            android:textSize="20dp"
            android:gravity="center"
            android:textColor="#fff"
            android:layout_marginLeft="60dp"></TextView>
    </com.example.framework.view.ToolBar>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/ji_she_height"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/ji_margin_left"
                android:text="基本设置"
                android:textColor="#38D13F"
                ></TextView>
        </LinearLayout>

    </ScrollView>

</LinearLayout>